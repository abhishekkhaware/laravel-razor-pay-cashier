{
    "name": "abhishekkhaware/laravel-razor-pay-cashier",
    "description": "Laravel Cashier provides an expressive, fluent interface to Razorpay's subscription billing services.",
    "keywords": ["laravel", "razorpay", "subscription", "billing"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/abhishekkhaware/laravel-razor-pay-cashier/issues",
        "source": "https://github.com/abhishekkhaware/laravel-razor-pay-cashier"
    },
    "authors": [
        {
            "name": "Abhishek Khaware",
            "email": "abhishekkhaware@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.31",
        "razorpay/razorpay": "^2.2",
        "symfony/http-kernel": "^5.1"
    },
    "require-dev": {
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "mockery/mockery": "^1.4.2",
        "vlucas/phpdotenv": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Abhishek\\Razorpay\\Cashier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abhishek\\Razorpay\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
